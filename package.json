{
  "name": "@storybook/testing-vue3",
  "version": "1.0.0",
  "description": "Testing utilities that allow you to reuse your stories in your unit tests",
  "keywords": [
    "storybook-addons",
    "style",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/testing-vue3"
  },
  "author": "yannbf@gmail.com",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite dev",
    "build": "rimraf dist && tsc && vite build",
    "test": "yarn build && cd ./example && yarn test",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn build && auto shipit"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@storybook/client-logger": "^7.6.0",
    "@storybook/preview-api": "^7.6.0",
    "@storybook/types": "^7.6.0",
    "@storybook/vue3": "^7.6.0",
    "auto": "^10.20.4",
    "concurrently": "^6.0.0",
    "vite-plugin-dts":"^3.6.4",
    "husky": "^5.1.3",
    "size-limit": "^4.10.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vitest": "^1.0.1",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "@storybook/client-logger": "^7.6.0",
    "@storybook/preview-api": "^7.6.0",
    "@storybook/types": "^7.6.0",
    "@storybook/vue3": "^7.6.0",
    "vue": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
